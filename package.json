    "@playwright/test": "^1.44.0",
    "@types/node": "^20.12.12",
    "babel-jest": "^29.7.0",
    "eslint": "^9.34.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mongodb-memory-server": "^10.2.0",
    "sonarqube-scanner": "^3.3.0",
    "typescript-eslint": "^8.41.0"
  }
}
